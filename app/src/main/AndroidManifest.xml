<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nelson.caliplay">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />


    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/ZarakiActionBar"
        android:supportsRtl="true">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>

        <activity android:name=".UserProfile">

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>

        <activity android:name=".ListSports">


        </activity>

        <activity android:name=".UserPrint">

        </activity>

        <activity android:name=".test.TestFeedback">

        </activity>

        <activity
            android:name=".test.StartingTest"
            android:parentActivityName=".MainActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />

        </activity>


        <activity android:name=".YouTube">

        </activity>

        <activity
            android:name=".timer.ReadyTimer"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name=".timer.WorkoutTimer"
            android:screenOrientation="portrait">


        </activity>
        <activity
            android:name=".timer.RestTimer"
            android:screenOrientation="portrait">


        </activity>


    </application>

</manifest>
